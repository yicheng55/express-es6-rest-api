{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "indent": [
        "error",
        2
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.4",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.7.1",
    "debug": "^4.3.4",
    "express": "^4.13.3",
    "log4js": "^6.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.8.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.11",
    "resource-router-middleware": "^0.6.0",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.20"
  }
}
